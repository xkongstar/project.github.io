.myCoupon {
    background: $color-page;
    font-size: $fontsize-medium;
}

.rules {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    height: 72rpx;
    font-size: 24rpx;
    color: #666;
    padding-right: 38rpx;
}

.rules image {
    margin-right: 5rpx;
    width: 24rpx;
    height: 24rpx;
}

.coupon-item {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    margin: 0 auto 28rpx;
    width: 670rpx;
    height: 244rpx;
    padding: 0 31rpx 15.2rpx;
    box-sizing: border-box;
}

.coupon-item .coupon-item-t {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 115rpx;
    color: #fc9153;
}

.coupon-item .coupon-item-t .coupon-item-tl view:first-child {
    font-size: 36rpx;
}

.coupon-item .coupon-item-t .coupon-item-tl view:last-child {
    font-size: 22rpx;
}

.coupon-item .coupon-item-t .coupon-item-tr {
    display: flex;
    font-size: 24rpx;
}

.coupon-item .coupon-item-t .coupon-item-tr .font-big {
    font-size: 82rpx;
}

.coupon-item .coupon-item-t .coupon-item-tr .yuan {
    margin-left: 10rpx;
    padding-bottom: 25rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
}

.coupon-item .coupon-item-t .coupon-item-tr .coupon_num {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    transform: translate(10rpx,25rpx);
    background: url(//pt-starimg.didistatic.com/static/starimg/img/1522138139918xGfnwbHSTGY3beQ2SlV.png) no-repeat;
    background-size: 100% 100%;
    height: 36rpx;
    width: 88rpx;
}

.coupon-item .coupon-item-b {
    font-size: 22rpx;
    color: #999;
}

.no_coupon {
    width: 252rpx;
    height: 252rpx;
}

.no_coupon_text {
    font-size: 28rpx;
    color: #666;
    margin-top: 26rpx;
}

.no_coupon_wrap {
    padding-top: 302rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}